{
  "name": "boilerplate-react-native-with-expo",
  "version": "2.0.0",
  "repository": "https://github.com/tientran0019/boilerplate-react-native-with-expo",
  "description": "A boilerplate for a react-native app using expo",
  "author": "Tien Tran <tientran0019@gmail.com>",
  "keywords": [
    "expo",
    "boilerplate",
    "react-native",
    "redux",
    "redux-devtools-extension"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "dev": "expo start",
    "start": "APP_ENV=production expo start",
    "android": "expo start --android",
    "android:prod": "APP_ENV=production expo start --android",
    "ios": "expo start --ios",
    "ios:prod": "APP_ENV=production expo start --ios",
    "web": "expo start --web",
    "web:prod": "APP_ENV=production expo start --web",
    "eject": "expo eject",
    "clean-install": "rm -rf node_modules && npm cache clean --force && watchman watch-del-all && yarn",
    "debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn start",
    "reset": "expo r -c",
    "build:all": "APP_ENV=production eas build -p all",
    "build:ios": "APP_ENV=production eas build -p ios --non-interactive",
    "build:local:ios": "APP_ENV=production eas build -p ios --local --clear-cache",
    "build:android": "APP_ENV=production eas build --profile=production --platform=android",
    "build:local:android": "APP_ENV=production eas build --profile=production --platform=android --local --clear-cache",
    "build:inspect:android": "APP_ENV=production eas build:inspect --profile=production --platform=android -s post-build -o ../test/ --force --verbose",
    "build:web": "APP_ENV=production expo build:web",
    "push": "expo publish",
    "push:prod": "APP_ENV=production expo publish --release-channel production",
    "eas-build-pre-install": "npm install --save --legacy-peer-deps"
  },
  "dependencies": {
    "@expo/samples": "^3.0.3",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "8.2.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/elements": "^1.3.3",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@zellosoft/antd-react-native": "^5.0.4",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "expo": "~45.0.4",
    "expo-camera": "~12.2.0",
    "expo-constants": "~13.1.1",
    "expo-font": "~10.1.0",
    "expo-image-manipulator": "~10.3.1",
    "expo-linear-gradient": "~11.3.0",
    "expo-linking": "~3.1.0",
    "expo-local-authentication": "~12.2.0",
    "expo-secure-store": "~11.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-updates": "~0.13.1",
    "expo-web-browser": "~10.2.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "rc-field-form": "^1.26.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-circular-progress": "^1.3.7",
    "react-native-crypto-js": "^1.0.0",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-popover-view": "^5.0.1",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "react-native-webview": "11.18.1",
    "react-redux": "^7.2.8",
    "react-use": "^17.4.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "rn-range-slider": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "redux-devtools-extension": "^2.13.9"
  },
  "private": true
}
