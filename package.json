{
  "name": "boilerplate-react-native-with-expo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "repository": "https://github.com/tientran0019/boilerplate-react-native-with-expo",
  "description": "A boilerplate for a react-native app using expo",
  "author": "Tien Tran <tientran0019@gmail.com>",
  "keywords": [
    "expo",
    "boilerplate",
    "react-native",
    "redux",
    "redux-devtools-extension"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "expo start",
    "start": "NODE_ENV=production expo start",
    "android": "expo start --android",
    "android:prod": "NODE_ENV=production expo start --android",
    "ios": "expo start --ios",
    "ios:prod": "NODE_ENV=production expo start --ios",
    "web": "expo start --web",
    "web:prod": "NODE_ENV=production expo start --web",
    "eject": "expo eject",
    "clean-install": "rm -rf node_modules && npm cache clean --force && watchman watch-del-all && yarn",
    "debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn start",
    "reset": "expo r -c",
    "build:ios": "NODE_ENV=production expo build:ios --release-channel production",
    "build:android": "NODE_ENV=production expo build:android --release-channel production",
    "push": "expo publish",
    "push:prod": "NODE_ENV=production expo publish --release-channel production"
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^12.0.5",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "6.0.0",
    "@react-native-community/slider": "3.0.3",
    "@react-navigation/bottom-tabs": "^6.0.4",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.6",
    "color": "^4.0.1",
    "dayjs": "^1.10.6",
    "expo": "^42.0.3",
    "expo-asset": "~8.3.3",
    "expo-constants": "~11.0.1",
    "expo-font": "~9.2.1",
    "expo-linking": "~2.3.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-web-browser": "~9.2.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "rc-field-form": "^1.21.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-redux": "^7.2.4",
    "react-use": "^17.2.4",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "rn-pdf-reader-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "private": false
}
